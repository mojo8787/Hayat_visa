{"ast":null,"code":"import Navbar from '../components/layout/Navbar.vue';\nimport ProgressBar from '@/components/ProgressBar.vue';\nimport PersonalInfoForm from '../components/forms/PersonalInfoForm.vue';\nimport DocumentsForm from '../components/forms/DocumentsForm.vue';\nimport PaymentForm from '@/components/PaymentForm.vue';\nimport NextBackButtons from '../components/buttons/NextBackButtons.vue';\nimport Footer from '../components/layout/Footer.vue';\nexport default {\n  name: 'ApplicationFormPage',\n  components: {\n    Navbar,\n    ProgressBar,\n    PersonalInfoForm,\n    DocumentsForm,\n    PaymentForm,\n    NextBackButtons,\n    Footer\n  },\n  data() {\n    return {\n      currentStep: 1,\n      // Track the current step\n      formData: {\n        fullName: '',\n        dateOfBirth: '',\n        nationality: '',\n        passportNumber: '',\n        email: '',\n        documents: null,\n        // Documents placeholder\n        payment: null // Payment placeholder\n      }\n    };\n  },\n  methods: {\n    nextStep() {\n      // Increment step (only if validation passes)\n      if (this.validateStep()) {\n        this.currentStep++;\n      }\n    },\n    prevStep() {\n      // Decrement step if not at the first step\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    validateStep() {\n      // Basic validation for the first step (more rules can be added for other steps)\n      if (this.currentStep === 1 && !this.formData.fullName) {\n        alert(\"Full name is required.\");\n        return false;\n      }\n      return true;\n    },\n    submitForm() {\n      // Final form submission logic\n      console.log('Form Data:', this.formData);\n      alert('Form submitted successfully!');\n    }\n  }\n};","map":{"version":3,"names":["Navbar","ProgressBar","PersonalInfoForm","DocumentsForm","PaymentForm","NextBackButtons","Footer","name","components","data","currentStep","formData","fullName","dateOfBirth","nationality","passportNumber","email","documents","payment","methods","nextStep","validateStep","prevStep","alert","submitForm","console","log"],"sources":["/Users/Saham/Documents/Warani/src/pages/ApplicationFormPage.vue"],"sourcesContent":["<template>\n    <div class=\"application-form-page\">\n      <Navbar />\n      \n      <!-- Progress Bar for showing current step -->\n      <ProgressBar :currentStep=\"currentStep\" />\n      \n      <!-- Dynamic Form Step Rendering -->\n      <div v-if=\"currentStep === 1\">\n        <PersonalInfoForm \n          :fullName.sync=\"formData.fullName\"\n          :dateOfBirth.sync=\"formData.dateOfBirth\"\n          :nationality.sync=\"formData.nationality\"\n          :passportNumber.sync=\"formData.passportNumber\"\n          :email.sync=\"formData.email\"\n        />\n      </div>\n      \n      <div v-if=\"currentStep === 2\">\n        <DocumentsForm v-model=\"formData.documents\" />\n      </div>\n      \n      <div v-if=\"currentStep === 3\">\n        <PaymentForm v-model=\"formData.payment\" />\n      </div>\n      \n      <!-- Navigation Buttons -->\n      <NextBackButtons\n        :currentStep=\"currentStep\"\n        @next=\"nextStep\"\n        @back=\"prevStep\"\n        @submit=\"submitForm\"\n      />\n      \n      <Footer />\n    </div>\n  </template>\n  \n  <script>\n  import Navbar from '../components/layout/Navbar.vue'\n  import ProgressBar from '@/components/ProgressBar.vue'\n  import PersonalInfoForm from '../components/forms/PersonalInfoForm.vue'\n  import DocumentsForm from '../components/forms/DocumentsForm.vue'\n  import PaymentForm from '@/components/PaymentForm.vue'\n  import NextBackButtons from '../components/buttons/NextBackButtons.vue'\n  import Footer from '../components/layout/Footer.vue'\n  \n  export default {\n    name: 'ApplicationFormPage',\n    components: {\n      Navbar,\n      ProgressBar,\n      PersonalInfoForm,\n      DocumentsForm,\n      PaymentForm,\n      NextBackButtons,\n      Footer\n    },\n    data() {\n      return {\n        currentStep: 1, // Track the current step\n        formData: {\n          fullName: '',\n          dateOfBirth: '',\n          nationality: '',\n          passportNumber: '',\n          email: '',\n          documents: null,  // Documents placeholder\n          payment: null  // Payment placeholder\n        }\n      };\n    },\n    methods: {\n      nextStep() {\n        // Increment step (only if validation passes)\n        if (this.validateStep()) {\n          this.currentStep++;\n        }\n      },\n      prevStep() {\n        // Decrement step if not at the first step\n        if (this.currentStep > 1) {\n          this.currentStep--;\n        }\n      },\n      validateStep() {\n        // Basic validation for the first step (more rules can be added for other steps)\n        if (this.currentStep === 1 && !this.formData.fullName) {\n          alert(\"Full name is required.\");\n          return false;\n        }\n        return true;\n      },\n      submitForm() {\n        // Final form submission logic\n        console.log('Form Data:', this.formData);\n        alert('Form submitted successfully!');\n      }\n    }\n  }\n  </script>\n"],"mappings":"AAuCE,OAAOA,MAAK,MAAO,iCAAgC;AACnD,OAAOC,WAAU,MAAO,8BAA6B;AACrD,OAAOC,gBAAe,MAAO,0CAAyC;AACtE,OAAOC,aAAY,MAAO,uCAAsC;AAChE,OAAOC,WAAU,MAAO,8BAA6B;AACrD,OAAOC,eAAc,MAAO,2CAA0C;AACtE,OAAOC,MAAK,MAAO,iCAAgC;AAEnD,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,UAAU,EAAE;IACVR,MAAM;IACNC,WAAW;IACXC,gBAAgB;IAChBC,aAAa;IACbC,WAAW;IACXC,eAAe;IACfC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MAAE;MAChBC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,WAAW,EAAE,EAAE;QACfC,WAAW,EAAE,EAAE;QACfC,cAAc,EAAE,EAAE;QAClBC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE,IAAI;QAAG;QAClBC,OAAO,EAAE,IAAG,CAAG;MACjB;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT;MACA,IAAI,IAAI,CAACC,YAAY,CAAC,CAAC,EAAE;QACvB,IAAI,CAACX,WAAW,EAAE;MACpB;IACF,CAAC;IACDY,QAAQA,CAAA,EAAG;MACT;MACA,IAAI,IAAI,CAACZ,WAAU,GAAI,CAAC,EAAE;Q