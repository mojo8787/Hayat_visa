sonalInfo\n        ? this.formData.personalInfo.someProperty\n        : null;\n    }\n  },\n  methods: {\n    nextStep() {\n      if (this.$v.formData.$invalid) {\n        this.$v.formData.$touch();\n        alert(\"Please correct the errors before proceeding.\");\n      } else {\n        this.currentStep++;\n      }\n    },\n    prevStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    submitForm() {\n      this.$v.formData.$touch();\n      if (!this.$v.formData.$invalid) {\n        console.log('Form Data:', this.formData);\n        alert('Form submitted successfully!');\n      } else {\n        alert(\"Please correct the errors before submitting.\");\n      }\n    },\n    setFormData(data) {\n      this.formData = {\n        ...data,\n        personalInfo: data.personalInfo || {} // Ensure personalInfo exists\n      };\n    }\n  }\n}\n</script>\n"],"mappings":"AAgCA,OAAOA,MAAK,MAAO,iCAAgC;AACnD,OAAOC,WAAU,MAAO,8BAA6B;AACrD,OAAOC,gBAAe,MAAO,0CAAyC;AACtE,OAAOC,aAAY,MAAO,uCAAsC;AAChE,OAAOC,WAAU,MAAO,8BAA6B;AACrD,OAAOC,eAAc,MAAO,2CAA0C;AACtE,OAAOC,MAAK,MAAO,iCAAgC;AACnD,SAASC,QAAQ,EAAEC,KAAK,EAAEC,SAAQ,QAAS,0BAAyB;AAEpE,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,UAAU,EAAE;IACVX,MAAM;IACNC,WAAW;IACXC,gBAAgB;IAChBC,aAAa;IACbC,WAAW;IACXC,eAAe;IACfC;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE;QACRC,YAAY,EAAE,CAAC,CAAC;QAAE;QAClBC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE;MACX,CAAC;MACDC,UAAU,EAAE,CAAC,uBAAuB,EAAE,mBAAmB,EAAE,iBAAiB;IAC9E,CAAC;EACH,CAAC;EACDC,WAAW,EAAE;IACXL,QAAQ,EAAE;MACRC,YAAY,EAAE;QACZK,QAAQ,EAAE;UAAEb;QAAS,CAAC;QACtBc,WAAW,EAAE;UAAEd;QAAS,CAAC;QACzBe,WAAW,EAAE;UAAEf;QAAS,CAAC;QACzBgB,cAAc,EAAE;UAAEhB,QAAQ;UAAEE,SAAS,EAAEA,SAAS,CAAC,CAAC;QAAE,CAAC;QACrDD,KAAK,EAAE;UAAED,QAAQ;UAAEC;QAAM;MAC3B,CAAC;MACDQ,SAAS,EAAE;QACT;QACAA,SAAS,EAAE;UAAET,QAAQ;UAAEE,SAAS,EAAEA,SAAS,CAAC,CAAC;QAAE;MACjD,CAAC;MACDQ,OAAO,EAAE;QACP;QACAA,OAAO,EAAE;UAAEV;QAAS;MACtB;IACF;EACF,CAAC;EACDiB,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAA